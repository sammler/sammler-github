{
  "name": "sammler-strategy-github",
  "version": "0.0.6",
  "description": "Sammler strategy to collect GitHub content.",
  "keywords": [
    "github",
    "sammler",
    "sammler-strategy"
  ],
  "homepage": "https://github.com/sammler/sammler-strategy-github",
  "bugs": {
    "url": "https://github.com/sammler/sammler-strategy-github/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Stefan Walther",
    "url": "https://github.com/stefanwalther",
    "twitter": "waltherstefan"
  },
  "main": "./src/index.js",
  "repository": "sammler/sammler-strategy-github",
  "scripts": {
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "dc-dev-down": "docker-compose --f=./docker-compose.dev.yml down",
    "dc-dev-up": "docker-compose --f=./docker-compose.dev.yml up --build",
    "dc-down": "docker-compose down",
    "dc-up": "docker-compose up --build",
    "docs": "verb",
    "lint": "yarn run lint:src && yarn run lint:test",
    "lint:fix": "yarn run lint:src:fix && yarn run lint:test:fix",
    "lint:src": "eslint src",
    "lint:src:fix": "eslint src --fix",
    "lint:test": "eslint test",
    "lint:test:fix": "eslint test --fix",
    "start": "node ./src/api/index.js",
    "start:dev": "nodemon -L --watch ./src/api --exec yarn run start",
    "test": "yarn run lint:src && yarn run lint:test && yarn run test:integration",
    "test:integration": "mocha './test/integration/**/*.spec.js' --require './test/mocha.conf.js' --timeout 6000",
    "test:unit": "mocha './test/unit/**/*.spec.js' --require './test/mocha.conf.js'"
  },
  "dependencies": {
    "amqp-as-promised": "^4.1.0",
    "amqplib": "^0.5.1",
    "amqplib-sugar": "sammler/amqplib-sugar",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "debug": "^2.3.3",
    "express": "^4.14.0",
    "github": "^6.1.0",
    "http-status": "^0.2.3",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "mongoose": "^4.7.1",
    "mongoose-timestamp": "^0.6.0",
    "mongoose-unique-validator": "^1.0.3",
    "superagent": "^3.3.1",
    "superagent-promise": "^1.1.0",
    "winster": "^0.2.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.10.2",
    "eslint-config-sammler": "*",
    "eslint-plugin-mocha": "^4.7.0",
    "gulp-format-md": "*",
    "isparta": "^4.0.0",
    "mocha": "^3.1.2",
    "nock": "^9.0.2",
    "nyc": "^9.0.1",
    "sammler-base": "sammler/sammler-base",
    "sammler-jobs-service": "sammler/sammler-jobs-service",
    "sammler-scheduler-service": "sammler/sammler-scheduler-service",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "sinon-mongoose": "^1.3.0",
    "supertest": "^2.0.1"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": []
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ]
  },
  "winster": {
    "configFile": "./src/api/config/logger-config.js"
  }
}

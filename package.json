{
  "name": "sammler-middleware-github",
  "version": "0.0.3",
  "description": "Sammler middleware to collect GitHub content.",
  "keywords": [
    "github",
    "sammler",
    "sammler-middleware"
  ],
  "homepage": "https://github.com/sammler/sammler-middleware-github",
  "bugs": {
    "url": "https://github.com/sammler/sammler-middleware-github/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Stefan Walther",
    "url": "https://github.com/stefanwalther",
    "twitter": "waltherstefan"
  },
  "files": [
    "/dist"
  ],
  "main": "/dist/index.js",
  "repository": "sammler/sammler-middleware-github",
  "scripts": {
    "build": "gulp build",
    "docker-compose": "docker-compose up --build",
    "docker-compose-down": "docker-compose down",
    "dc-dev-up": "docker-compose --f=dc-dev.yml up --build",
    "dc-dev-down": "docker-compose --f=dc-dev.yml down",
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "docs": "verb",
    "lint": "eslint src",
    "lint:test": "eslint test",
    "start": "yarn run build && node ./dist/index.js",
    "test": "yar run test:unit && yarn run test:integration",
    "test:integration": "mocha './test/integration/**/*.spec.js' --compilers js:babel-core/register --require './test/mocha.conf.js' --timeout 6000",
    "test:unit": "mocha './test/unit/**/*.spec.js' --compilers js:babel-core/register --require './test/mocha.conf.js'"
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "babel-polyfill": "^6.16.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "github": "^6.1.0",
    "lodash": "^4.17.2",
    "mongoose": "^4.7.1",
    "mongoose-unique-validator": "^1.0.3",
    "winster": "^0.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.10.2",
    "eslint-config-idiomatic": "^2.1.0",
    "eslint-plugin-mocha": "^4.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-format-md": "*",
    "gulp-sourcemaps": "^2.2.0",
    "mocha": "^3.1.2",
    "nock": "^9.0.2",
    "nyc": "^9.0.1",
    "sinon": "^1.17.6",
    "sinon-as-promised": "^4.0.2",
    "sinon-mongoose": "^1.3.0",
    "supertest": "^2.0.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": []
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ]
  }
}
